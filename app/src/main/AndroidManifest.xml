<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.java.app45">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.java.app45.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.java.app45.DetailActivity" />
        <activity android:name="com.java.app45.SearchActivity" />
        <activity android:name="com.java.app45.TextDetailActivity" />
        <activity android:name="com.java.app45.SearchResultActivity" />
        <activity android:name="com.java.app45.HistoryActivity" />
        <activity android:name="com.java.app45.CollectionActivity"></activity>
    </application>

</manifest>